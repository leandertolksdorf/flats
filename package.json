{
  "name": "flats",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "generate-types": "source .env && npx openapi-typescript $SUPABASE_URL/rest/v1/?apikey=$SUPABASE_KEY --output types/supabase.ts",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-picker/picker": "2.1.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/elements": "^1.2.1",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@supabase/supabase-js": "^1.25.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "expo": "~43.0.0",
    "expo-app-loading": "~1.2.1",
    "expo-font": "~10.0.3",
    "expo-linear-gradient": "~10.0.3",
    "expo-status-bar": "~1.1.0",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^7.19.5",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-global-props": "^1.1.5",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-snackbar-component": "^1.1.12",
    "react-native-svg": "12.1.1",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "0.17.1",
    "tailwind-rn": "^4.0.0",
    "validator": "^13.7.0",
    "zustand": "^3.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-global-props": "^1.1.1",
    "@types/react-native-snackbar-component": "^1.1.1",
    "@types/validator": "^13.7.0",
    "concurrently": "^7.0.0",
    "tailwindcss": "^3.0.15",
    "typescript": "~4.3.5"
